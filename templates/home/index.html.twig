{% extends 'base.html.twig' %}

{% block title %}Hello HomeController!{% endblock %}

{% block body %}
<div class="container my-3">
    <div class="row">
        {% for pokemon in pagination %}
        <div class="col-12 col-md-6 col-lg-4">
            <h3>{{ pokemon.name }}</h3>
            <img src="{{pokemon.picture}}" class="img-fluid" alt="">
        </div>
        {% endfor %}
    </div>
</div>
{% endblock %}